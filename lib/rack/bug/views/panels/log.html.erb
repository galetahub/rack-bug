<h3>Log Messages</h3>
<table>
  <thead>
    <tr>
      <th>Level</th>
      <th>Time</th>
      <th>Message</th>
      <th>Location</th>
    </tr>
  </thead>
  <tbody>
    <% i = 1 %>
    <% logs.each do |log| %>
      <tr class="<%= i % 2 == 0 ? "even" : "odd" %>">
        <td><%= log[0] %></td>
        <td></td>
        <td><%= log[1] %></td>
        <td><%#= log[1].to_s.gsub(/\e\[[;\d]+m/, "") %></td>
      </tr>
      <% i += 1 %>
    <% end %>
  </tbody>
</table>